#include <iostream>

class MyClass {
private:
    int x;
    int y;

public:
    // Constructor with initializer list
    MyClass(int a, int b) : x(a), y(b) {
        // Other constructor code if needed
    }

    void printValues() {
        std::cout << "x = " << x << ", y = " << y << std::endl;
    }
};

int main() {
    // Create an object of MyClass using initializer list
    MyClass myObject(42, 99);

    // Call member function to print the values
    myObject.printValues();

    return 0;
}
